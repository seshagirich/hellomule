	<api-gateway:autodiscovery apiId="${api.id}" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="859819b2-56bb-42f8-956b-b82c401d63b0" flowRef="hellomuleFlow" />
	
</mule>
